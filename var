# Supprimer la DACL
vserver security file-directory ntfs dacl remove -vserver cats-svm -ntfs-sd CATS_LINUX_SD -account 'BUILTIN\Administrators' -access-type allow


# Supprimer le SD
vserver security file-directory ntfs delete -vserver cats-svm -ntfs-sd CATS_LINUX_SD

# VÃ©rifier
vserver security file-directory ntfs show -vserver cats-svm
vserver security file-directory ntfs dacl show -vserver cats-svm
