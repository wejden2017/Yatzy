ansible-playbook \
  -i inventory/aws_ec2_inventory.py \
  playbooks/install_vtom_server.yml \
  --vault-password-file=.vault_pass.txt \
  -e "env=poc do_install=true do_uninstall=false" \
  -e ansible_ssh_common_args='-o StrictHostKeyChecking=no' \
  -e ansible_ssh_private_key_file=~/.ssh/poc_ec2_keypair1.pem \
  -e ansible_python_interpreter=/usr/bin/python3


-e "env=poc do_install=false do_uninstall=true"
