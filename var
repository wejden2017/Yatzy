if (camt004.getStatus() != null) {
    log.info("Setting param 6 {}", camt004.getStatus().getValue());
    StatementHelper.setNullableInteger(statement, 6, camt004.getStatus().getValue());
} else {
    log.info("Setting param 6 is NULL");
    StatementHelper.setNullableInteger(statement, 6, null);
}
