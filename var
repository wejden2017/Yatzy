for reservation in response['Reservations']:
    for instance in reservation['Instances']:
        print("==== DEBUG KEYS ====")
        print(instance.keys())  # Pour voir les champs de niveau Instance
        
        if 'NetworkInterfaces' in instance:
            print("==== DEBUG NETWORK INTERFACES ====")
            for ni in instance['NetworkInterfaces']:
                print(ni.keys())  # Pour voir les champs d'une interface
                if 'VpcId' in ni:
                    print("VPC trouv√© dans NI:", ni['VpcId'])

