-- Privilèges système de jerome
SELECT privilege 
FROM dba_sys_privs 
WHERE grantee = 'JEROME'
ORDER BY privilege;

-- Privilèges sur objets de jerome
SELECT owner, table_name, privilege 
FROM dba_tab_privs 
WHERE grantee = 'JEROME'
ORDER BY owner, table_name;

-- Rôles de jerome
SELECT granted_role 
FROM dba_role_privs 
WHERE grantee = 'JEROME';
