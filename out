ALTER QMGR CHLAUTH(DISABLED)
REFRESH SECURITY TYPE(CONNAUTH)
REFRESH SECURITY TYPE(AUTHSERV)
ALTER CHANNEL(DEV.APP.SVRCONN) CHLTYPE(SVRCONN) MCAUSER('admin')
DISPLAY CHANNEL(DEV.APP.SVRCONN) MCAUSER
SET AUTHREC PROFILE('**') OBJTYPE(QMGR) PRINCIPAL('admin') AUTHADD(ALL)
SET AUTHREC PROFILE('**') OBJTYPE(QUEUE) PRINCIPAL('admin') AUTHADD(ALL)
REFRESH SECURITY TYPE(AUTHSERV)


DISPLAY QMGR CHLAUTH
DISPLAY CHANNEL(DEV.APP.SVRCONN)
DISPLAY AUTHREC PRINCIPAL('admin')
