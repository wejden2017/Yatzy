SELECT
  s.sid,
  s.serial#,
  s.username,
  s.status,
  s.osuser,
  s.machine,
  s.program,
  s.module,
  s.logon_time,
  s.sql_id,
  s.event
FROM v$session s
WHERE s.type = 'USER'
  AND s.username IS NOT NULL
  AND s.status = 'ACTIVE'
ORDER BY s.username, s.machine, s.sid;

ALTER SYSTEM DISCONNECT SESSION '45,59307' IMMEDIATE;
ALTER SYSTEM DISCONNECT SESSION '55,2913'  IMMEDIATE;
ALTER SYSTEM DISCONNECT SESSION '237,65316' IMMEDIATE;

ALTER SESSION SET ddl_lock_timeout = 600;

ALTER PACKAGE LUCA.PACK_GESTION_ACCES COMPILE SPECIFICATION;
ALTER PACKAGE LUCA.PACK_GESTION_ACCES COMPILE BODY;


